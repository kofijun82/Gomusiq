{
  "name": "gomusiq",
  "version": "1.0.0",
  "description": "Centralized scripts for Gomusiq project",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all start:backend start:frontend",
    "start:backend": "npm --prefix backend start",
    "start:frontend": "npm --prefix frontend run dev",
    "dev": "npm-run-all dev:backend dev:frontend",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "build": "npm-run-all build:frontend",
    "build:frontend": "npm --prefix frontend run build",
    "seed:songs": "npm --prefix backend run seed:songs",
    "seed:albums": "npm --prefix backend run seed:albums"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.21.0"
  }
}
